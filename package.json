{
  "name": "metadeck",
  "version": "1.0.0",
  "description": "Plugin for viewing metadata for non steam games from TheGamesDB. Part of the EmuDeck Project",
  "scripts": {
    "build": "shx rm -rf dist && rollup -c",
    "watch": "rollup -c -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package": "dbuild build",
    "start": "dbuild deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EmuDeck/MetaDeck.git"
  },
  "keywords": [
    "decky",
    "plugin",
    "plugin-template",
    "steam-deck",
    "deck"
  ],
  "author": "Witherking25 <witherking@withertech.com>",
  "license": "GPL-3",
  "bugs": {
    "url": "https://github.com/EmuDeck/MetaDeck/issues"
  },
  "homepage": "https://github.com/EmuDeck/MetaDeck#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/lodash-es": "^4.14.184",
    "@types/react": "16.14.0",
    "@types/webpack": "^5.28.0",
    "deckbuild": "^1.0.9",
    "react-router": "^6.4.3",
    "rollup": "^2.77.1",
    "rollup-plugin-import-assets": "^1.1.1",
    "shx": "^0.3.4",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "decky-frontend-lib": "^3.9.0",
    "fastest-levenshtein": "^1.0.16",
    "localforage": "^1.10.0",
    "lodash-es": "^4.17.21",
    "mobx": "5",
    "react-icons": "^4.4.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom"
      ]
    }
  }
}
